{
  "app": {
    "name": "Builda Website"
  },
  "modules": {
    "blueprint": {
      "component": "3.0.0",
      "page": "1.0.0"
    }
  },
  "commands": {
    "atom": {
      "type": "blueprint",
      "outputPath": "./components/atoms",
      "use": "component"
    },
    "molecule": {
      "type": "blueprint",
      "outputPath": "./components/molecules",
      "use": "component"
    },
    "organism": {
      "type": "blueprint",
      "outputPath": "./components/organisms",
      "use": "component"
    },
    "partial": {
      "type": "blueprint",
      "outputPath": "./components/partials",
      "use": "component"
    },
    "page": {
      "type": "blueprint",
      "outputPath": "./pages",
      "use": "page"
    },
    "input": {
      "type": "blueprint",
      "outputPath": "./components/inputs",
      "use": "component"
    }
  }
}
